{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "max-len": [
      "error",
      {
        "code": 150
      }
    ],
    "no-unused-expressions": [
      "error",
      {
        "allowTernary": true
      }
    ],
    "linebreak-style": 0,
    "no-console": "off",
    "consistent-return": "off"
  }
}
